{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ccadfstrg"
		},
		"adls_ls_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'adls_ls'"
		},
		"sqldb_ls_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'sqldb_ls'"
		},
		"adb_kv_ls_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://cc-azure-proj.vault.azure.net/"
		},
		"adls_ls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://ccadfstrg.dfs.core.windows.net/"
		},
		"api_ls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/"
		},
		"sqldb_ls_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "tk-azure-sql-server.database.windows.net"
		},
		"sqldb_ls_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "CCpresentationlayer"
		},
		"sqldb_ls_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sqladmin"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ccadfstrg_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"adls_ls_accountKey": {
						"value": "[parameters('adls_ls_accountKey')]"
					},
					"sqldb_ls_password": {
						"value": "[parameters('sqldb_ls_password')]"
					},
					"adb_kv_ls_properties_typeProperties_baseUrl": {
						"value": "[parameters('adb_kv_ls_properties_typeProperties_baseUrl')]"
					},
					"adls_ls_properties_typeProperties_url": {
						"value": "[parameters('adls_ls_properties_typeProperties_url')]"
					},
					"api_ls_properties_typeProperties_url": {
						"value": "[parameters('api_ls_properties_typeProperties_url')]"
					},
					"sqldb_ls_properties_typeProperties_server": {
						"value": "[parameters('sqldb_ls_properties_typeProperties_server')]"
					},
					"sqldb_ls_properties_typeProperties_database": {
						"value": "[parameters('sqldb_ls_properties_typeProperties_database')]"
					},
					"sqldb_ls_properties_typeProperties_userName": {
						"value": "[parameters('sqldb_ls_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}